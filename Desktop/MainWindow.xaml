<Window x:Class="Desktop.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:Desktop" mc:Ignorable="d" DataContext="{Binding Main, Source={StaticResource Locator}}" Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Label Name="MyLabel" Content="{Binding Path=Hello}" DockPanel.Dock="Top"/>
        <Label Name="MyLabel2" Content="baff" DockPanel.Dock="Top"/>

    </DockPanel>
</Window>
